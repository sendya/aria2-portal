{
  "name": "aria-portal",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "dev:cache": "vite --host --force",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx,js,json,vue} --fix",
    "lint:check": "eslint src/**/*.{ts,tsx,js,json,vue}"
  },
  "dependencies": {
    "@ant-design-vue/pro-layout": "^3.0.0-beta.11",
    "@ant-design-vue/use": "^0.0.1-alpha.10",
    "@ant-design/icons-vue": "^6.0.1",
    "ant-design-vue": "^2.2.0-rc.1",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.1",
    "vue": "^3.1.0",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^2.1.2",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^15.6.2",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.1.0",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "husky": "4",
    "less": "^4.1.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.1.3",
    "vite": "^2.3.5",
    "vue-tsc": "^0.0.24"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --cache --fix"
    ]
